<div class="page-header">
    <h1>Market <small> Show All </small></h1>
</div>
<div> 
 <p>
    <table class="condensed"> 
        <thead>
            <tr> 
                <th>Price</th>           
                <th>VolRemaining</th>    
                <th>Item</th>          
                <th>Range</th>           
                <th>OrderID</th>  
                <th>Bid</th>
                <th>StationID</th>
                <th>SolarSystemID</th>
                <th>Jumps</th>      
            </tr>
        </thead>
        <tbody>
            <% orders.forEach(function(order) { %>
                    <tr>
                        <td> <%= order["price"] %> </td>
                        <td> <%= order["volRemaining"] %> </td>
                        <td> <%= order["item_name"] %> </td>
                        <td> <%= order["range"] %> </td>
                        <td> <%= order["orderID"] %> </td> <td> <%= order["bid"] %> </td>
                        <td> <%= order["stationID"] %> </td>
                        <td> <%= order["solarSystemID"] %> </td>
                        <td> <%= order["jumps"] %> </td>   
                    </tr>
            <% }); %>
        <tbody>
    </table>
 </p>
</div>
